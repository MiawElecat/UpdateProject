	<queryString>
		<![CDATA[SELECT
    pembelian.tanggal_pembelian AS pembelian_tanggal_pembelian,
    pembelian.total_harga AS pembelian_total_harga,
    distributor.id_distributor AS distributor_id_distributor,
    distributor.nama_distributor AS distributor_nama_distributor,
    pembelian.id_pembelian AS pembelian_id_pembelian
FROM
    pembelian pembelian
INNER JOIN
    distributor distributor ON pembelian.id_distributor = distributor.id_distributor
WHERE
    pembelian.tanggal_pembelian BETWEEN $P{tgl1} AND $P{tgl2}
]]>
	</queryString>
